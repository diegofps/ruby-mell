<%-
  validate_variable :property

  type = property.fetch(:type, :int)
  name = property.fetch(:name, :Id)
  visibility = property.fetch(:visibility, :public)
  virtual = if property.fetch(:virtual, false) then ' virtual' else '' end
  display_name = property.fetch(:display_name, nil)
-%>
<%- unless display_name.nil? -%>
[Display(Name="<%= display_name %>")]
<%- end -%>
<%= "#{visibility}#{virtual} #{type} #{name} { get; set; }" %>
