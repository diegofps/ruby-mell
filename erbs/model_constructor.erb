<%-
  validate_variable :entity
-%>
public <%= entity.get(:name) -%>()
{
  <%- entity.get(:properties).each do |prop| -%>
    <%- if (prop.has_key?(:hasMany)) -%>
    <%= cammel_case(prop.get(:hasMany).get(:plural)) %> = new List<<%= prop.get(:hasMany).get(:name) %>>();
    <%- end -%>
  <%- end -%>
}
